{
  "name": "neotool-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3000",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "format": "prettier --write .",
    "msw:init": "msw init public --save",
    "test": "vitest run -c vitest.config.ts",
    "test:watch": "vitest -c vitest.config.ts",
    "test:coverage": "vitest run --coverage -c vitest.config.ts",
    "e2e": "playwright test -c playwright.config.mjs",
    "e2e:ui": "playwright test --ui -c playwright.config.mjs",
    "e2e:install": "playwright install --with-deps",
    "ci:unit": "pnpm run typecheck && pnpm run lint -- --max-warnings=0 && pnpm run test",
    "ci:e2e": "pnpm run e2e",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck:tests": "tsc -p tsconfig.vitest.json --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^3.10.0",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-date-pickers": "^8.11.2",
    "@sentry/nextjs": "8.25.0",
    "@tanstack/react-query": "^5.87.4",
    "ag-grid-community": "^34.2.0",
    "ag-grid-react": "^34.2.0",
    "dayjs": "^1.11.18",
    "i18next": "23.11.5",
    "i18next-browser-languagedetector": "7.2.1",
    "i18next-http-backend": "2.5.2",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "14.1.2",
    "react-input-mask": "^2.0.4",
    "react-is": "^18.3.1",
    "react-number-format": "^5.4.4",
    "stylis": "^4.3.6",
    "stylis-plugin-rtl": "^2.1.1",
    "zod": "^3.25.76",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.46.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "8.2.9",
    "@storybook/react-vite": "^8.6.14",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "20.14.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.7.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "jsdom": "^24.0.0",
    "msw": "^2.4.0",
    "prettier": "^3.6.2",
    "storybook": "^8.6.14",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.43.0",
    "vite": "^5.4.20",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.6.0"
  },
  "overrides": {
    "react-is": "^18.3.1",
    "@playwright/test": "^1.46.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "pnpm@10.16.0"
}
